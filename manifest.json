{
  "manifest_version": 2,

  "name": "Postikiisu",
  "version": "1.0.0",

  "permissions": [
    "http://www.postimees.ee/"
  ],
  "icons": { "48": "icon-48.png",
    "128": "icon-128.png" },
  "content_scripts": [
    {
      "matches": ["*://*.postimees.ee/*"],
      "js": ["postikiisu.js"]
    }
  ]
}
